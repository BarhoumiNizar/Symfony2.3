<div class="col-lg-12">
                        <div class="table-responsive">
                            <table class="table table-bordered table-hover table-striped">
                                <thead>
                                    <tr>      
                                        <th>id</th>
                                        <th>nom</th>
                                        <th>prenom</th>
                                        <th>domaine</th>
                                        <th>niveau</th>
                                        <th>email</th>
                                        <th>telephone</th>
                                        <th>roles</th>
                                        <th>Status</th>
                                        <th>Enable/Disable</th>
                                        <th>Delete</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    {% for user in APPRENANT %}
                                        {% if user is not null %}
                                     <tr class="success">
                                        <td>{{ user.id }}</td>
                                        <td>{{ user.nom }}</td>
                                        <td>{{ user.prenom }}</td>
                                        <td>{{ user.domaine }}</td>
                                        <td>{{ user.niveau }}</td>
                                        <td>{{ user.email }}</td>
                                        <td>{{ user.telephone }}</td>
                                        <td>APPRENANT</td>
                                        <td>{% if user.enabled %}Enable{% else %}Disable{% endif %}</td>
                                        <td>
                                               {% if not user.enabled %}<a href="javascript:loader('/PiMoocWeb1/web/app_dev.php/admin/enableuser/{{ user.id }}');"><button type="button"  class="btn btn-success">Enable</button></a>
                                               {% else %} <a href="javascript:loader('/PiMoocWeb1/web/app_dev.php/admin/disableuser/{{ user.id }}');"><button type="button" class="btn btn-warning">Disable</button></a>
                                        {% endif %}
                                        </td>
                                        
                                         <td><button type="button" name="id:{{ user.id }} | name and lastname:{{ user.nom }} {{ user.prenom }}"  class="btn btn-danger" data-href="javascript:loader('{{ path('mooc_admin_back_offie_DeleteUser',{'id':user.id})}}');" data-toggle="modal" data-target="#confirm-delete">Delete</button></td>
                                     </tr>
                                    {% endif %}
                                    {% endfor %}
                                             {% for user in   FORMATEUR  %}
                                                 {% if user is not null %}
